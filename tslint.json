{
    "extends": "tslint:all",
    "rules": {
        "no-inferrable-types": false,
        "no-implicit-dependencies": [
            true,
            "dev"
        ],
        "comment-format": false,
        "no-magic-numbers": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}